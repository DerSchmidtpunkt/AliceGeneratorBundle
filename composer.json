{
    "name": "trappar/alice-generator-bundle",
    "description": "Symfony2 bundle to generate Alice fixtures from existing Doctrine entities",
    "keywords": ["Symfony", "Fixture", "ORM", "Alice"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Way",
            "email": "jeff.way@me.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lafourchette/DoctrineBundle"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/finder": "^2.7|~3.0",
        "symfony/validator": "~2.3|~3.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/yaml": "~2.3|~3.0",
        "symfony/phpunit-bridge": "^2.7.4|~3.0",
        "sensio/generator-bundle": "^3.0",
        "phpunit/phpunit": "5.4.*",
        "hautelook/alice-bundle": "^1.3",
        "doctrine/data-fixtures": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Trappar\\AliceGeneratorBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Trappar\\AliceGeneratorBundle\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
