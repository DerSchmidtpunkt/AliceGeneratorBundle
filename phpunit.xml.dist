    <php>
        <server name="KERNEL_DIR" value="tests/Trappar/AliceGeneratorBundle/SymfonyApp" />
    </php>

    <testsuites>
        <testsuite name="AliceGeneratorBundle">
            <directory suffix="Test.php">./tests</directory>
